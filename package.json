    "@types/node": "^20.8.0",
    "@types/react": "^18.2.69",
    "@types/react-dom": "^18.2.22",
    "@types/react-input-mask": "^3.0.4",
    "@types/ws": "^8.5.10",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.50.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.6",
    "npm-run-all": "^4.1.5",
    "tsx": "^3.12.3",
    "typescript": "^4.9.3",
    "vite-node": "^1.4.0",
    "vite-plugin-html": "^3.2.2",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "dependencies": {
