{
  "name": "home-control",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prerun": "echo \"\\\"$(date +'%Y.%m.%d-%H:%M:%S')\\\"\" > src/version.json",
    "postbuild": "cp src/robots.txt ./dist && cp src/version.json dist/latest.json",
    "dev": "yarn prerun && vite",
    "build": "yarn prerun && yarn lint && rm -rf dist && vite build",
    "lint": "tsc && eslint --ext .ts,.tsx src",
    "serve": "serve dist --single -p 3000"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^6.2.0",
    "@mui/x-date-pickers": "^7.23.2",
    "@react-input/mask": "^2.0.4",
    "@tanstack/query-sync-storage-persister": "^5.62.7",
    "@tanstack/react-query": "^5.62.7",
    "@tanstack/react-query-devtools": "^5.62.7",
    "@tanstack/react-query-persist-client": "^5.62.7",
    "@uidotdev/usehooks": "^2.4.1",
    "@uiw/react-color-wheel": "^2.3.4",
    "home-assistant-js-websocket": "^9.4.0",
    "humanize-duration": "^3.32.1",
    "jotai": "^2.12.3",
    "ky": "^1.7.3",
    "lodash": "^4.17.21",
    "macy": "^2.5.1",
    "mouse-event-offset": "^3.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "serve": "^14.2.4",
    "wouter": "^3.1.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-react": "^4.2.1",
    "dotenv": "^16.4.7",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.6",
    "typescript": "^5.2.2",
    "vite": "^5.2.10",
    "vite-plugin-pwa": "^0.20.0",
    "vite-tsconfig-paths": "^5.1.4",
    "workbox-core": "^7.1.0",
    "workbox-window": "^7.1.0"
  },
  "resolutions": {
    "sharp": "0.32.6",
    "sharp-ico": "0.1.5"
  }
}
